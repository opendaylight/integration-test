{
    "bgp-inet:ipv4-routes": {
        "ipv4-route": [
            {
                "attributes": {
                    "as-path": {
                        "segments": [
                            {
                                "as-sequence": [
                                    $AS_NUMBER,
                                    $PEER_AS
                                ]
                            }
                        ]
                    },
                    "ipv4-next-hop": {
                        "global": "192.0.2.1"
                    },
                    "origin": {
                        "value": "igp"
                    }
                },
                "path-id": 0,
                "prefix": "$PREFIXLEN",
                "route-key": "$PREFIXLEN"
            }
        ]
    }
}
